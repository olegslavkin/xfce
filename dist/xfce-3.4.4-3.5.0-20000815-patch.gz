+  gdk_draw_arc(window, gc1, FALSE, x, y, width+1, height+1, 45 * 64, 225 * 64);
+  gdk_draw_arc(window, gc2, FALSE, x, y, width+1, height+1, 225 * 64, 180 * 64);
