diff -Naur -x *.gmo xfce-3.4.4/xfce/diagnostic.c xfce-3.5.0/xfce/diagnostic.c
--- xfce-3.4.4/xfce/diagnostic.c	Thu Jan  1 01:00:00 1970
+++ xfce-3.5.0/xfce/diagnostic.c	Mon Aug 14 21:26:39 2000
@@ -0,0 +1,163 @@
+/*  gxfce
